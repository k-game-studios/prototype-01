{
  "name": "prototype-01",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "src/config/electron/main.js",
  "description": "Descrição do seu aplicativo", 
  "scripts": {
    "start:dev": "vite",
    "start:dev:desktop": "electron . --enable-accelerated-2d-canvas --enable-gpu-rasterization --disable-software-rasterizer",
    "build": "tsc && vite build",
    "build:desktop": "electron-builder",
    "preview": "vite preview"
  },
  "devDependencies": {
    "electron": "^35.0.0",
    "electron-builder": "^25.1.8",
    "typescript": "~5.7.2",
    "vite": "^6.2.0"
  },
  "dependencies": {
    "phaser": "^3.88.2"
  },
  "build": {
    "appId": "com.exemplo.app",
    "productName": "Nome do seu app",
    "files": [
      "dist/**/*",
      "src/config/electron/main.js"
    ],
    "directories": {
      "output": "build-desktop"
    }
  }
}
